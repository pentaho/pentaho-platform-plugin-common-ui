{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "package-res/resources/web/pentaho/i18n.js",
      "package-res/resources/web/pentaho/service.js",
      "package-res/resources/web/pentaho/_doc",
      "package-res/resources/web/pentaho/lang",
      "package-res/resources/web/pentaho/data",
      "package-res/resources/web/pentaho/util",
      "package-res/resources/web/pentaho/type",
      "package-res/resources/web/pentaho/visual/_doc",
      "package-res/resources/web/pentaho/visual/base",
      "package-res/resources/web/pentaho/visual/color"
    ],
    "exclude": [],
    "includePattern": ".+\\.js(doc)?$"
  },
  "plugins": ["plugins/markdown", "customPlugins/amd"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "template": "../build-res/jsdoc-template",
    "encoding": "utf8",
    "destination": "doc-js/vizapi",
    "recurse": true,
    "private": true,
    "verbose": true
  }
}