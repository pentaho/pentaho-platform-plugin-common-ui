<?js
var list = obj;
?>

<table class="api-ref-table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Summary</th>
        </tr>
    </thead>

    <tbody>
        <?js
        var self = this;
        if(list) {
          list.forEach(function(item) {
            if(!item) { return; }
            var isClass = item.kind === "class";
            ?>
            <tr>
              <td> <?js= self.linkto(item.longname, item.parsedName) ?> </td>
              <td> <?js= (isClass ? item.classSummary : item.summary) ?> </td>
            </tr>
        <?js }); 
        }?>           
    </tbody>
</table>

