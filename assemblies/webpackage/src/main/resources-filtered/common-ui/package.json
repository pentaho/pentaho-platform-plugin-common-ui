        "base": "element"
      },
      "pentaho/data/filter/abstract": {
        "base": "complex"
      },
      "pentaho/data/filter/true": {
        "alias": "true",
        "base": "pentaho/data/filter/abstract"
      },
      "pentaho/data/filter/false": {
        "alias": "false",
        "base": "pentaho/data/filter/abstract"
