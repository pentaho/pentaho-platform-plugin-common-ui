      },
      "pentaho/visual/models/pie": {
        "base": "pentaho/visual/models/abstract"
      },
      "pentaho/visual/models/heatGrid": {
        "base": "pentaho/visual/models/abstract"
      },
      "pentaho/visual/models/sunburst": {
        "base": "pentaho/visual/models/abstract"
      },
      "pentaho/visual/models/donut": {
        "base": "pentaho/visual/models/abstract"
