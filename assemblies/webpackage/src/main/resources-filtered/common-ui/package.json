      },
      "pentaho/type/list": {
        "alias": "list",
        "base": "value"
      },
      "pentaho/type/element": {
        "alias": "element",
        "base": "value"
      },
      "pentaho/type/complex": {
        "alias": "complex",
        "base": "element"
      },
      "pentaho/type/application": {
        "alias": "application",
        "base": "complex"
      },
      "pentaho/type/model": {
        "alias": "model",
        "base": "complex"
      },
      "pentaho/type/simple": {
        "alias": "simple",
        "base": "element"
      },
      "pentaho/type/number": {
        "alias": "number",
