        "base": "pentaho/visual/role/adaptation/Strategy",
        "ranking": -5
      },
      "pentaho/visual/role/adaptation/IdentityStrategy": {
        "base": "pentaho/visual/role/adaptation/Strategy",
        "ranking": -10
      },
      "pentaho/visual/role/adaptation/TupleStrategy": {
        "base": "pentaho/visual/role/adaptation/Strategy",
        "ranking": -20
