  "map": {
    "common-ui": {
      "css": "common-ui/util/require-css/css",
      "underscore": "common-ui/underscore"
    }
  },

  "packages": [
    {"name": "pentaho/module", "main": "metaOf"},
    {"name": "pentaho/environment"},
    {"name": "pentaho/i18n", "main": "defaultService"},
    {"name": "pentaho/debug", "main": "manager"}
  ],

  "config": {
    "pentaho/modules": {
      "pentaho/config/spec/IRuleSet": {
        "base": null,
        "isAbstract": true
      },
      "pentaho/type/Instance": {
        "alias": "instance",
        "base": null
      },
      "pentaho/type/Value": {
        "alias": "value",
        "base": "instance"
      },
      "pentaho/type/Property": {
        "alias": "property",
        "base": "instance"
      },
      "pentaho/type/List": {
        "alias": "list",
        "base": "value"
      },
      "pentaho/type/Element": {
        "alias": "element",
        "base": "value"
      },
      "pentaho/type/Complex": {
        "alias": "complex",
        "base": "element"
      },
      "pentaho/type/Simple": {
        "alias": "simple",
        "base": "element"
      },
      "pentaho/type/Number": {
        "alias": "number",
        "base": "simple"
      },
      "pentaho/type/String": {
        "alias": "string",
        "base": "simple"
      },
      "pentaho/type/Boolean": {
        "alias": "boolean",
        "base": "simple"
      },
